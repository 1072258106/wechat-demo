.user-wrap {
  background:#ebeaea;
}

.order-wrap {
  margin: 10rpx 0;
  background: #fff;
}

.user-infor-wrap {
  width: 750rpx;
  height: 320rpx;
  color: #999;
  display: flex;
  flex-direction: column;
  align-items: center;
  background:#fff;
}

.user-img {
  overflow: hidden;
  display: block;
  width: 160rpx;
  height: 160rpx;
  margin: 20rpx;
  margin-top: 50rpx;
  border-radius: 50%;
  border: 2px solid #fff;
  box-shadow: 3px 3px 10px rgba(0, 0, 0, 0.2);
}

.myorder {
  font-size: 30rpx;
  vertical-align: middle;
}

.order-type {
  display: flex;
  height: 150rpx;
  text-align: center;
  font-size: 26rpx;
}

.order-item {
  margin: 0 auto;
  padding-top: 20rpx;
  flex-direction: row;
}

.order-item image {
  width: 56rpx;
  height: 52rpx;
}

.center-list {
  margin-bottom: 10rpx;
	padding: 0 10rpx;
	background: #fff;
	border-top: 1px solid #eee;
	border-bottom: 1px solid #eee;
	line-height: 1;
}

.list-son {
	width: 100%;
	height: 2.4rem;
  padding-left: 26rpx;
	border-bottom: 1px solid #eee;
	color: #444;
	font-size: 0.7rem;
  display: flex;
  flex-direction: row;
  align-items: center;
}

.list-son:last-child {
	border-bottom: 0;
}

.list-son image {
	width: 40rpx;
  height: 40rpx;
	margin-right: 20rpx;
}

.list-son .arrow {
  display: flex;
  flex-grow: 1;
  justify-content: flex-end;
  padding-right: 36rpx;
}

.arrow image {
  width: 20rpx;
  height: 20rpx;
}

.unlogin-wrap {
  background: #fff;
  display: flex;
  flex-direction: row;
  justify-content: center;
  padding: 30rpx 0;
}

.unlogin {
  width: 96%;
}
